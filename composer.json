{
    "name": "hannesvdvreken/guzzle-clockwork",
    "description": "A Guzzle middleware that logs requests with Clockwork",
    "authors": [
        {
            "name": "Hannes Van De Vreken",
            "email": "vandevreken.hannes@gmail.com"
        }
    ],
    "keywords": ["Clockwork", "log", "Guzzle", "middleware", "profiling", "profiler", "PHP", "browser", "Devtools"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "hannesvdvreken/guzzle-profiler": "^1.0",
        "itsgoingd/clockwork": "^1.4|^2.0|^3.0|^4.0"
    },
    "require-dev": {
        "illuminate/support": "~4,<5.8.0",
        "phpunit/phpunit": "^5.6.3||^8.2.3",
        "friendsofphp/php-cs-fixer": "^2.2.20"
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttp\\Profiling\\Clockwork\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttp\\Profiling\\Clockwork\\Unit\\": "tests/unit/"
        }
    },
    "config": {
        "discard-changes": true
    }
}
