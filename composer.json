{
    "name": "hannesvdvreken/guzzle-clockwork",
    "description": "A Guzzle plugin that listens to events and logs them with Clockwork",
    "authors": [
        {
            "name": "Hannes Van De Vreken",
            "email": "vandevreken.hannes@gmail.com"
        }
    ],
    "keywords": ["Clockwork", "Log", "Guzzle", "Events", "PHP", "Browser", "Devtools"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~4|~5"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "itsgoingd/clockwork": "1.5.*",
        "illuminate/support": "~4"
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttp\\Subscriber\\Log\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
